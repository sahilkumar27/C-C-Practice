#include <bits/stdc++.h>

using namespace std;

void plusMinus(vector<int> arr) {
    int n=arr.size();
    float p=0,q=0,r=0;
    for(int i=0;i<n;i++)
    {
        if(arr[i]>0)
        {
            p++;
        }
        else if(arr[i]<0)
        {
            q++;
        }
        else{
            r++;
        }
    }
    cout<<p/n<<endl;
    cout<<q/n<<endl;
    cout<<r/n<<endl;

}

